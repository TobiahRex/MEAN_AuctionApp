{
	"extends": "airbnb",
	"rules": {
		"no-underscore-dangle": ["error", { "allow": ["_id"] }],
		"no-param-reassign": ["error", { "props": false }]
	},
	"globals": {
		"$": false,
		"newUserObj._Password": true,
		"UserObj._Password": true,
		"savedUser._Password": true,
		"dbUser._id": true,
		"angular": true,
		"User": true,
		"Comment": true
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"jquery": true
	}
}
